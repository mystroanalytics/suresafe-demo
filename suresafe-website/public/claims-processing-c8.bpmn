<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_ClaimsProcessing" targetNamespace="http://suresafe.com/bpmn/claims" exporter="Camunda Modeler" exporterVersion="5.40.1">
  <bpmn:message id="Message_DocumentUploaded" name="DocumentUploaded">
    <bpmn:extensionElements>
      <zeebe:subscription correlationKey="=claimId" />
    </bpmn:extensionElements>
  </bpmn:message>
  <bpmn:message id="Message_SignatureComplete" name="SignatureComplete">
    <bpmn:extensionElements>
      <zeebe:subscription correlationKey="=claimId" />
    </bpmn:extensionElements>
  </bpmn:message>
  <bpmn:process id="Process_ClaimsProcessing" name="SureSafe Claims Processing" isExecutable="true">
    <bpmn:startEvent id="Start_ClaimReceived" name="Claim Received">
      <bpmn:outgoing>Flow_ToCreateFolder</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="Flow_ToCreateFolder" sourceRef="Start_ClaimReceived" targetRef="Task_CreateBoxFolder" />
    <bpmn:serviceTask id="Task_CreateBoxFolder" name="Create Box Claim Folder">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="box-create-folder" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=userName" target="userName" />
          <zeebe:output source="=boxFolderId" target="boxFolderId" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToCreateFolder</bpmn:incoming>
      <bpmn:outgoing>Flow_ToBoxAIExtract</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToBoxAIExtract" sourceRef="Task_CreateBoxFolder" targetRef="Task_BoxAIExtract" />
    <bpmn:serviceTask id="Task_BoxAIExtract" name="Box AI: Extract Claim Data">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="box-ai-extract" />
        <zeebe:ioMapping>
          <zeebe:input source="=boxFolderId" target="folderId" />
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:output source="=extractedData" target="extractedData" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToBoxAIExtract</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCalculateRisk</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToCalculateRisk" sourceRef="Task_BoxAIExtract" targetRef="Task_CalculateRiskScore" />
    <bpmn:serviceTask id="Task_CalculateRiskScore" name="Calculate Risk Score">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="risk-scoring" />
        <zeebe:ioMapping>
          <zeebe:input source="=extractedData" target="extractedData" />
          <zeebe:input source="=claimType" target="claimType" />
          <zeebe:input source="=estimatedAmount" target="estimatedAmount" />
          <zeebe:output source="=riskScore" target="riskScore" />
          <zeebe:output source="=riskFactors" target="riskFactors" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToCalculateRisk</bpmn:incoming>
      <bpmn:outgoing>Flow_ToRiskGateway</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToRiskGateway" sourceRef="Task_CalculateRiskScore" targetRef="Gateway_RiskLevel" />
    <bpmn:exclusiveGateway id="Gateway_RiskLevel" name="Risk Level?">
      <bpmn:incoming>Flow_ToRiskGateway</bpmn:incoming>
      <bpmn:outgoing>Flow_LowRisk</bpmn:outgoing>
      <bpmn:outgoing>Flow_MediumRisk</bpmn:outgoing>
      <bpmn:outgoing>Flow_HighRisk</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_LowRisk" name="Low (0-40)" sourceRef="Gateway_RiskLevel" targetRef="Task_AutoAssignAdjuster">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=riskScore &lt;= 40</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_MediumRisk" name="Medium (41-75)" sourceRef="Gateway_RiskLevel" targetRef="Task_SupervisorReview">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=riskScore &gt; 40 and riskScore &lt;= 75</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_HighRisk" name="High (76+)" sourceRef="Gateway_RiskLevel" targetRef="Task_ReferToSIU">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=riskScore &gt; 75</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Task_AutoAssignAdjuster" name="Auto-Assign Adjuster">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="adjuster-assignment" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=claimType" target="claimType" />
          <zeebe:input source="=riskScore" target="riskScore" />
          <zeebe:output source="=adjusterId" target="adjusterId" />
          <zeebe:output source="=adjusterName" target="adjusterName" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_LowRisk</bpmn:incoming>
      <bpmn:outgoing>Flow_LowToDocRequest</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="Task_SupervisorReview" name="Supervisor Review">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="Claims_Supervisors" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:supervisorReviewForm" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_MediumRisk</bpmn:incoming>
      <bpmn:outgoing>Flow_SupervisorDecision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_SupervisorDecision" sourceRef="Task_SupervisorReview" targetRef="Gateway_SupervisorDecision" />
    <bpmn:exclusiveGateway id="Gateway_SupervisorDecision" name="Supervisor Decision">
      <bpmn:incoming>Flow_SupervisorDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_SupervisorApproved</bpmn:outgoing>
      <bpmn:outgoing>Flow_SupervisorEscalate</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_SupervisorApproved" name="Approve" sourceRef="Gateway_SupervisorDecision" targetRef="Task_ManualAssignAdjuster">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=reviewDecision = "approve"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_SupervisorEscalate" name="Escalate to SIU" sourceRef="Gateway_SupervisorDecision" targetRef="Task_ReferToSIU">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=reviewDecision = "escalate"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Task_ManualAssignAdjuster" name="Assign Selected Adjuster">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="adjuster-assignment" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=selectedAdjusterId" target="adjusterId" />
          <zeebe:output source="=adjusterId" target="adjusterId" />
          <zeebe:output source="=adjusterName" target="adjusterName" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_SupervisorApproved</bpmn:incoming>
      <bpmn:outgoing>Flow_ManualToDocRequest</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_ReferToSIU" name="Refer to SIU">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="siu-referral" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=riskScore" target="riskScore" />
          <zeebe:input source="=riskFactors" target="riskFactors" />
          <zeebe:output source="=siuCaseId" target="siuCaseId" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_HighRisk</bpmn:incoming>
      <bpmn:incoming>Flow_SupervisorEscalate</bpmn:incoming>
      <bpmn:outgoing>Flow_ToInvestigation</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToInvestigation" sourceRef="Task_ReferToSIU" targetRef="Task_InvestigatorReview" />
    <bpmn:userTask id="Task_InvestigatorReview" name="SIU Investigation">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="SIU_Investigators" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:investigatorForm" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToInvestigation</bpmn:incoming>
      <bpmn:outgoing>Flow_InvestigationDecision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_InvestigationDecision" sourceRef="Task_InvestigatorReview" targetRef="Gateway_FraudOutcome" />
    <bpmn:exclusiveGateway id="Gateway_FraudOutcome" name="Investigation Outcome">
      <bpmn:incoming>Flow_InvestigationDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_FraudConfirmed</bpmn:outgoing>
      <bpmn:outgoing>Flow_FraudCleared</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_FraudConfirmed" name="Fraud Confirmed" sourceRef="Gateway_FraudOutcome" targetRef="Task_DenyClaim">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=investigationOutcome = "FRAUD_CONFIRMED"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_FraudCleared" name="Cleared" sourceRef="Gateway_FraudOutcome" targetRef="Task_PostSIUAssign">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=investigationOutcome = "CLEARED"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Task_PostSIUAssign" name="Assign Adjuster (Post-SIU)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="adjuster-assignment" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=claimType" target="claimType" />
          <zeebe:output source="=adjusterId" target="adjusterId" />
          <zeebe:output source="=adjusterName" target="adjusterName" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_FraudCleared</bpmn:incoming>
      <bpmn:outgoing>Flow_PostSIUToDocRequest</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_LowToDocRequest" sourceRef="Task_AutoAssignAdjuster" targetRef="Task_RequestDocuments" />
    <bpmn:sequenceFlow id="Flow_ManualToDocRequest" sourceRef="Task_ManualAssignAdjuster" targetRef="Task_RequestDocuments" />
    <bpmn:sequenceFlow id="Flow_PostSIUToDocRequest" sourceRef="Task_PostSIUAssign" targetRef="Task_RequestDocuments" />
    <bpmn:serviceTask id="Task_RequestDocuments" name="Request Supporting Documents">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="document-request" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=userId" target="userId" />
          <zeebe:input source="=claimType" target="claimType" />
          <zeebe:output source="=documentRequestId" target="documentRequestId" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_LowToDocRequest</bpmn:incoming>
      <bpmn:incoming>Flow_ManualToDocRequest</bpmn:incoming>
      <bpmn:incoming>Flow_PostSIUToDocRequest</bpmn:incoming>
      <bpmn:outgoing>Flow_ToWaitDocs</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToWaitDocs" sourceRef="Task_RequestDocuments" targetRef="Event_WaitForDocuments" />
    <bpmn:intermediateCatchEvent id="Event_WaitForDocuments" name="Wait for Documents">
      <bpmn:incoming>Flow_ToWaitDocs</bpmn:incoming>
      <bpmn:incoming>Flow_MoreDocsNeeded</bpmn:incoming>
      <bpmn:outgoing>Flow_DocsReceived</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageDef_DocUploaded" messageRef="Message_DocumentUploaded" />
    </bpmn:intermediateCatchEvent>
    <bpmn:sequenceFlow id="Flow_DocsReceived" sourceRef="Event_WaitForDocuments" targetRef="Task_ProcessDocuments" />
    <bpmn:serviceTask id="Task_ProcessDocuments" name="Process Uploaded Documents">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="box-ai-extract" />
        <zeebe:ioMapping>
          <zeebe:input source="=boxFolderId" target="folderId" />
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:output source="=extractedData" target="newExtractedData" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_DocsReceived</bpmn:incoming>
      <bpmn:outgoing>Flow_ToAdjusterReview</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToAdjusterReview" sourceRef="Task_ProcessDocuments" targetRef="Task_AdjusterReview" />
    <bpmn:userTask id="Task_AdjusterReview" name="Adjuster Claim Review">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition assignee="=adjusterId" candidateGroups="Claims_Adjusters" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:adjusterReviewForm" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToAdjusterReview</bpmn:incoming>
      <bpmn:outgoing>Flow_AdjusterDecision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_AdjusterDecision" sourceRef="Task_AdjusterReview" targetRef="Gateway_AdjusterDecision" />
    <bpmn:exclusiveGateway id="Gateway_AdjusterDecision" name="Adjuster Decision">
      <bpmn:incoming>Flow_AdjusterDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_ApproveSettlement</bpmn:outgoing>
      <bpmn:outgoing>Flow_DenyFromAdjuster</bpmn:outgoing>
      <bpmn:outgoing>Flow_MoreDocsNeeded</bpmn:outgoing>
      <bpmn:outgoing>Flow_EscalateForApproval</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_ApproveSettlement" name="Approve" sourceRef="Gateway_AdjusterDecision" targetRef="Gateway_ApprovalLevel">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=adjusterDecision = "approve"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_DenyFromAdjuster" name="Deny" sourceRef="Gateway_AdjusterDecision" targetRef="Task_DenyClaim">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=adjusterDecision = "deny"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_MoreDocsNeeded" name="More Docs" sourceRef="Gateway_AdjusterDecision" targetRef="Event_WaitForDocuments">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=adjusterDecision = "request_more_docs"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_EscalateForApproval" name="Escalate" sourceRef="Gateway_AdjusterDecision" targetRef="Task_ManagerApproval">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=adjusterDecision = "escalate"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:exclusiveGateway id="Gateway_ApprovalLevel" name="Approval Authority">
      <bpmn:incoming>Flow_ApproveSettlement</bpmn:incoming>
      <bpmn:outgoing>Flow_AdjusterCanApprove</bpmn:outgoing>
      <bpmn:outgoing>Flow_NeedsManager</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_AdjusterCanApprove" name="Under $50K" sourceRef="Gateway_ApprovalLevel" targetRef="Task_GenerateSettlement">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=recommendedAmount &lt; 50000</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_NeedsManager" name="Over $50K" sourceRef="Gateway_ApprovalLevel" targetRef="Task_ManagerApproval">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=recommendedAmount &gt;= 50000</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="Task_ManagerApproval" name="Manager Approval">
      <bpmn:extensionElements>
        <zeebe:assignmentDefinition candidateGroups="Executives" />
        <zeebe:formDefinition formKey="camunda-forms:bpmn:managerApprovalForm" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_NeedsManager</bpmn:incoming>
      <bpmn:incoming>Flow_EscalateForApproval</bpmn:incoming>
      <bpmn:outgoing>Flow_ManagerDecision</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_ManagerDecision" sourceRef="Task_ManagerApproval" targetRef="Gateway_ManagerDecision" />
    <bpmn:exclusiveGateway id="Gateway_ManagerDecision" name="Manager Decision">
      <bpmn:incoming>Flow_ManagerDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_ManagerApproved</bpmn:outgoing>
      <bpmn:outgoing>Flow_ManagerDenied</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_ManagerApproved" name="Approved" sourceRef="Gateway_ManagerDecision" targetRef="Task_GenerateSettlement">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=managerDecision = "approved"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_ManagerDenied" name="Denied" sourceRef="Gateway_ManagerDecision" targetRef="Task_DenyClaim">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=managerDecision = "deny"</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:serviceTask id="Task_GenerateSettlement" name="Generate Settlement Documents">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="generate-settlement" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=recommendedAmount" target="settlementAmount" />
          <zeebe:input source="=boxFolderId" target="boxFolderId" />
          <zeebe:output source="=settlementDocId" target="settlementDocId" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_AdjusterCanApprove</bpmn:incoming>
      <bpmn:incoming>Flow_ManagerApproved</bpmn:incoming>
      <bpmn:outgoing>Flow_ToBoxSign</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToBoxSign" sourceRef="Task_GenerateSettlement" targetRef="Task_BoxSignRequest" />
    <bpmn:serviceTask id="Task_BoxSignRequest" name="Send Box Sign Request">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="box-sign-request" />
        <zeebe:ioMapping>
          <zeebe:input source="=settlementDocId" target="documentId" />
          <zeebe:input source="=userId" target="signerId" />
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:output source="=signRequestId" target="signRequestId" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToBoxSign</bpmn:incoming>
      <bpmn:outgoing>Flow_ToWaitSign</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToWaitSign" sourceRef="Task_BoxSignRequest" targetRef="Event_WaitForSignature" />
    <bpmn:intermediateCatchEvent id="Event_WaitForSignature" name="Wait for Signature">
      <bpmn:incoming>Flow_ToWaitSign</bpmn:incoming>
      <bpmn:outgoing>Flow_SignatureReceived</bpmn:outgoing>
      <bpmn:messageEventDefinition id="MessageDef_Signed" messageRef="Message_SignatureComplete" />
    </bpmn:intermediateCatchEvent>
    <bpmn:sequenceFlow id="Flow_SignatureReceived" sourceRef="Event_WaitForSignature" targetRef="Task_ProcessPayment" />
    <bpmn:serviceTask id="Task_ProcessPayment" name="Process Payment">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="payment-processing" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=recommendedAmount" target="paymentAmount" />
          <zeebe:input source="=userId" target="payeeId" />
          <zeebe:output source="=paymentConfirmation" target="paymentConfirmation" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_SignatureReceived</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCloseClaim</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToCloseClaim" sourceRef="Task_ProcessPayment" targetRef="Task_CloseClaim" />
    <bpmn:serviceTask id="Task_CloseClaim" name="Close Claim">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="close-claim" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=paymentConfirmation" target="paymentConfirmation" />
          <zeebe:output source="=closedAt" target="closedAt" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_ToCloseClaim</bpmn:incoming>
      <bpmn:outgoing>Flow_ToSuccessEnd</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToSuccessEnd" sourceRef="Task_CloseClaim" targetRef="End_ClaimPaid" />
    <bpmn:endEvent id="End_ClaimPaid" name="Claim Paid">
      <bpmn:incoming>Flow_ToSuccessEnd</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:serviceTask id="Task_DenyClaim" name="Generate Denial Letter">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="generate-denial" />
        <zeebe:ioMapping>
          <zeebe:input source="=claimId" target="claimId" />
          <zeebe:input source="=denialReason" target="reason" />
          <zeebe:input source="=boxFolderId" target="boxFolderId" />
          <zeebe:output source="=denialDocId" target="denialDocId" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_DenyFromAdjuster</bpmn:incoming>
      <bpmn:incoming>Flow_ManagerDenied</bpmn:incoming>
      <bpmn:incoming>Flow_FraudConfirmed</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCloseAsDenied</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_ToCloseAsDenied" sourceRef="Task_DenyClaim" targetRef="End_ClaimDenied" />
    <bpmn:endEvent id="End_ClaimDenied" name="Claim Denied">
      <bpmn:incoming>Flow_ToCloseAsDenied</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Process_ClaimsProcessing">
      <bpmndi:BPMNShape id="Start_ClaimReceived_di" bpmnElement="Start_ClaimReceived">
        <dc:Bounds x="152" y="282" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="132" y="325" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_CreateBoxFolder_di" bpmnElement="Task_CreateBoxFolder">
